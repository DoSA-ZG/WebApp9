@model Projekt

<tr hx-target="this" hx-swap="outerHTML">
    <td class="text-left">@Model.ImeProjekta</td>
    <td class="text-left">@Model.PocetakProjekta.ToString("dd/M/yyyy")</td>
    <td class="text-left">@Model.KrajProjekta.ToString("dd/M/yyyy")</td>
    <td class="text-left">@Model.Oznaka</td>
    <td class="text-left">@Model.VrstaProjekta.Naziv</td>
    <td class="text-left">@Model.Racun.Iban</td>
    <td class="text-left">@Model.Opis</td>
</tr>