@model Posao

<tr hx-target="this" hx-swap="outerHTML">
    <td class="text-left">@Model.Projekt.ImeProjekta</td>
    <td class="text-left">@Model.Naziv</td>
    <td class="text-left">@Model.OcekivaniPocetak.ToString("dd/M/yyyy")</td>
    <td class="text-left">@Model.OcekivaniZavrsetak.ToString("dd/M/yyyy")</td>
    <td class="text-left">@Model.Budzet</td>
    <td class="text-left">@Model.VrstaPosla.Naziv</td>
</tr>