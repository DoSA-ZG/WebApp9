@model IEnumerable<ZaduzenaOsoba>

<table class="table table-striped" id="table-osobe">
    <thead>
        <tr>
            @{
                var count = 0;
                string[] nazivi = { "Ime", "Prezime", "E-mail", "Telefon", "IBAN"};
                for (int i = 1; i <= nazivi.Length; i++)
                {
                   <th class="text-left">
                        @nazivi[i - 1]
                   </th>
                }
                    <th></th>
            }
        <tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            @foreach (var zaduzenik in Model)
            {
                var dateTime = DateTime.Now;
                var autocompleteKey = dateTime.ToString() + dateTime.Nanosecond + dateTime.Millisecond;
                                <tr>
                                    <td class="text-left" id="idColumn">
                                        <input type="hidden" id="count" value="@count" />
                                <input name="ZaduzenaOsoba[@count].Osoba.ImeOsobe" value="@zaduzenik.Osoba.ImeOsobe" />
                                <input id="idValue" name="ZaduzenaOsoba[@count].IdOsoba" value="@zaduzenik.IdOsoba" type="hidden"/>
                            </td>
                            <td class="text-left">
                                        <input name="ZaduzenaOsoba[@count].Osoba.PrezimeOsobe" value="@zaduzenik.Osoba.PrezimeOsobe"  />
                            </td>
                            <td class="text-left">
                                        <input name="ZaduzenaOsoba[@count].Osoba.Email" value="@zaduzenik.Osoba.Email"  />
                            </td>
                            <td class="text-left">
                                        <input name="ZaduzenaOsoba[@count].Osoba.Telefon" value="@zaduzenik.Osoba.Telefon"  />
                            </td>
                            <td class="text-left">
                                        <input name="ZaduzenaOsoba[@count].Osoba.Iban" value="@zaduzenik.Osoba.Iban"  />
                            </td>
                            <td>
                                <button type="button" class="btn btn-sm btn-danger deleterow" title="Izbaci"><i class="fa fa-minus"></i></button>
                            </td>
                        </tr>
                ++count;
            }
        }

        <tr>
            <td class="text-left col-sm-1">
                <input id="osobe-ime"/>
                <input id="zadnji-count" value="@count" hidden="hidden" />
            </td>
            <td class="text-left col-sm-1">
                <input id="osobe-prezime"/>
            </td>
            <td class="text-left col-sm-1">
                <input id="osobe-email"/>
            </td>
            <td class="text-left col-sm-1">
                <input id="osobe-telefon"/>
            </td>
            <td class="text-left col-sm-1">
                <input id="osobe-iban"/>
            </td>
            <td>
                <button id="osoba-dodaj" class="btn btn-sm btn-secondary" title="Dodaj"><i class="fas fa-plus"></i></button>
            </td>
        </tr>
    </tbody>
</table>