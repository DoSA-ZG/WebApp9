@model Zadatak

<tr hx-target="this" hx-swap="outerHTML">
    <td class="text-left">@Model.Naziv</td>
    <td class="text-left">@Model.VrstaZadatka.NazivVrsteZdtk</td>
    <td class="text-left">@Model.Zahtjev.Naziv</td>
    <td class="text-left">@Model.VrijemeIsporuke.ToString("dd/M/yyyy")</td>
    <td class="text-left">@Model.StupanjDovrsenosti</td>
    <td class="text-left">@Model.Prioritetnost</td>
</tr>
